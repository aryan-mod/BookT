PORT=5000
NODE_ENV=development

# Database
MONGO_URI=mongodb+srv://<username>:<password>@cluster.mongodb.net/<dbname>?retryWrites=true&w=majority

# JWT (never commit real values)
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_ACCESS_EXPIRE=15m
REFRESH_TOKEN_EXPIRE_DAYS=7
REFRESH_TOKEN_COOKIE_NAME=refreshToken

# CORS - frontend origin for credential-based auth (required for HttpOnly cookies)
FRONTEND_URL=http://localhost:5174
# Optional: extra origins (comma-separated, include protocol). Default includes 5173-5176, 3000
CORS_ORIGIN=http://localhost:5173,http://localhost:5174,http://localhost:5175,http://localhost:5176,http://localhost:3000

# Rate limiting - auth routes (login, register, refresh) per 15 min
AUTH_RATE_LIMIT_MAX=10

GOOGLE_CLIENT_ID=6297xxxxx.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your_secret_here